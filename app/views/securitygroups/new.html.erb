<div class="headlabel" align="center">
<h1>New securitygroup</h1>

<% form_for(@securitygroup) do |f| %>
<table class="mainTable" id="widthControl1">
	<tr><td>
		<table class="tableList" id="noTrHover">
			<tr id="firstRow">
				<td><div align="left"><%= link_to 'Back', securitygroups_path %></div></td>
				<td><div align="center"><%= f.error_messages %></div></td>
			</tr>
			<tr><td colspan="2">
				<table class="tableDetail">
					<tr>
						<td> <%= f.label :name %></td>
						<td id="secondTd"><%= f.text_field :name %></td>
					</tr>
				</table>
			</td></tr>
			<tr><td align="right" colspan="2">
				<%= f.submit "Create" %>
			</td></tr>
		</table>
	</td></tr>
</table>
<% end %>
</div>

