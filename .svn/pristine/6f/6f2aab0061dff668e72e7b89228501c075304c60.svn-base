	color: #000;
	text-align: center
}

tbody th {
	padding: 20px 10px
}

tbody tr.odd {
	background-color: #F7F7F7;
	color: #777
}

tbody a {
	padding: 1px 2px;
	color: #333;
	text-decoration: none;
	border-bottom: 0px dotted #E63C1E;
}

caption a, caption a:visited, caption a:hover {
    color: #000;
    font-weight: bold;
    font-size: 10px;
    text-transform: uppercase;
    text-align: left
}

th a, th a:visited{
    color: #000;
    font-weight: bold;
    font-size: 9pt;
    text-transform: uppercase;
    text-align: left
}

th a:hover{
    color: #000;
    font-weight: bold;
    font-size: 9pt;
    text-decoration: underline;
    text-align: left

}

tbody a:active,tbody a:hover,tbody a:focus,tbody a:visited {
	color: #666
}

tbody td+td+td+td a {
	color: #000;
	font-weight: bold;
	border-bottom: 0;
        text-decoration: underline; 
}
