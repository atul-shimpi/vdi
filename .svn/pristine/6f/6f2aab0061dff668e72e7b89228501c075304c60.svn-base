	border-right: 1px solid #CCC;
	border-left: 1px solid #CCC;
        border-bottom: 1px solid #CCC;
        border-top: 1px solid #CCC;
}
caption {
	border-left: 0;
	padding: 10px;
}
td {
	border-left: 0;
	padding: 5px;
}

td b {
    font-weight: bold;
    text-transform: uppercase
}
caption {
    font-size: 12px;
}

caption,th, thead th,tfoot th,tfoot td {
	background-color: #CCC;
	color: #000;
	font-weight: bold;
}

th, thead th,tfoot th,tfoot td {
	text-transform: uppercase
}

