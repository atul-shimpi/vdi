<div class="headlabel" align="center">
<h1>Editing securitygroup</h1>
<% form_for(@securitygroup) do |f| %>
  <%= f.error_messages %>
  <table class="mainTable" id="widthControl1">
  	<tr><td>
		<table class="tableList" id="noTrHover">
			<tr id="firstRow">
				<td><div align="left"><%= link_to 'Back', securitygroups_path %></div></td>
				<td><div align="center"><%= link_to 'SelectPort', :controller => "securitygroups", :action => "selectport", :id => @securitygroup %></div></td>
				<td><div align="right"><%= link_to 'Show', @securitygroup %></div></td>
			</tr>
			<tr><td colspan="3">
				<table class="tableDetail">
					<tr>
						<td><%= f.label :name %></td>
						<td id="secondTd"><%= f.text_field :name %></td>
					</tr>
				</table>
			</td></tr>
			<tr><td align="right" colspan="3">
				 <%= f.submit "Update" %>
			</td></tr>
		</table>
	</td></tr>
  </table>
<% end %>
</div>
